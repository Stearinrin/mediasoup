include "transport.fbs";
include "worker.fbs";

namespace FBS.Request;

enum Method: uint8 {
  WORKER_CLOSE = 0,
  WORKER_DUMP,
  WORKER_GET_RESOURCE_USAGE,
  WORKER_UDATE_SETTINGS,
  TRANSPORT_CONSUME
}

union Body {
  FBS.Transport.ConsumeRequest,
  FBS.Worker.UpdateableSettings,
}

table Request {
  id:uint32;
  method:Method;
  handler_id:string;
  body:Body;
}

root_type Request;
