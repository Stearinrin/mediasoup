include "transport.fbs";
include "worker.fbs";

namespace FBS.Request;

enum Method: uint8 {
  WORKER_CLOSE = 0,
  WORKER_DUMP,
  TRANSPORT_CONSUME
}

union Body {
  FBS.Transport.ConsumeRequest,
}

table Request {
  id:uint32;
  method:Method;
  handler_id:string;
  body:Body;
}

root_type Request;
