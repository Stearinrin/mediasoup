
namespace FBS.SctpParameters;

table NumSctpStreams {
  os:uint32 = 1024;
  mis:uint32 = 1024;
}

table SctpParameters {
  // Port is always 5000.
  port:uint16=5000;
  os:uint16;
  mis:uint16;
  max_message_size:uint32;
  send_buffer_size:uint32;
  sctp_buffered_amount:uint32;
  is_data_channel:bool;
}


